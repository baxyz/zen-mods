{
  "name": "tab-context-organizer",
  "displayName": "Tab Context Menu Organizer",
  "version": "1.0.0",
  "description": "Simple tab context menu organizer that lets you show/hide different sections based on your preferences",
  "author": "baxyz",
  "license": "AGPL-3.0-or-later",
  "compatibility": {
    "zen": ">=1.15.0"
  },
  "features": [
    "Simple preference-based organization of tab context menu",
    "Show/hide tab movement options (move to start/end, move to new window)",
    "Show/hide workspace movement options",
    "Show/hide device sync options",
    "Show/hide container options"
  ],
  "files": {
    "userChrome": "style.css",
    "preferences": "preferences.json"
  },
  "preferences": {
    "enabled": true,
    "configurable": [
      "mod.tab_context_organizer.show_tab_movement",
      "mod.tab_context_organizer.show_workspace",
      "mod.tab_context_organizer.show_device",
      "mod.tab_context_organizer.show_container"
    ]
  },
  "tags": [
    "context-menu",
    "tabs",
    "organization"
  ],
  "marketplace": {
    "submission_ready": true,
    "screenshot": "screenshot.png",
    "zen_store_compatible": true,
    "css_only": true
  }
}